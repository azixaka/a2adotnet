{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "A2Adotnet.Server": "Debug"
    }
  },
  "AllowedHosts": "*",
  "AgentCard": {
    "Name": "A2A Sample .NET Agent",
    "Description": "A sample agent demonstrating the A2A protocol with .NET",
    "Url": "http://localhost:5123/a2a",
    "Version": "1.0.0",
    "Provider": {
      "Organization": "A2Adotnet Samples"
    },
    "Capabilities": {
      "Streaming": true,
      "PushNotifications": false
    },
    "Authentication": null,
    "DefaultInputModes": [ "text" ],
    "DefaultOutputModes": [ "text" ],
    "Skills": [
      {
        "Id": "echo",
        "Name": "Echo Skill",
        "Description": "Echoes back the input text.",
        "Examples": [ "Echo this message" ]
      },
      {
        "Id": "joke",
        "Name": "Joke Skill",
        "Description": "Tells a simple joke.",
        "Examples": [ "Tell me a joke" ]
      },
       {
        "Id": "long-story",
        "Name": "Long Story (Streaming)",
        "Description": "Tells a short story via SSE streaming.",
        "Examples": [ "Tell me a story (stream)" ]
      }
    ]
  }
}